config {
  data {
    split_percent: 0.85
    directory_pattern: '/home/tanderson/datasets/cfb/cfbstats-com-2006*'
  }
  data_augment {
    randomizer_func: 'stochastically_randomize_half_vector'
    rate: 2
  }
  train {
    save_checkpoints_steps: 100
    model_dir: '/home/tanderson/git/cfb-predictor/model_out'
    eval_throttle_secs: 1
    train_steps: 3000
    batch_size: 20
    learning_rate: 0.1
  }
  hidden_unit {
    neurons: 7
    activation {
      type: 'relu'
    }
  }
  dropout {
    rate: 0.5
  }
  output_layer {
    neurons: 2
  }
}